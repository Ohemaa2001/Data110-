        "df_sorted = df.sort_values(by=(\"Offense\"), ascending=True)\n",
        "plt.figure(figsize=(10, 10))\n",
        "plt.barh(df_sorted[\"Offense\"], df_sorted[\"Offenses per 100,000 People (2023)\"])\n",
        "plt.xlabel(\"Offense\")\n",
        "plt.ylabel(\"Offenses per 100,000 People (2023)\")\n",
        "plt.title('Offenses per crime')\n",
        "\n",
        "ax = plt.gca()\n",
        "ax.tick_params(axis='y', labelsize=8)\n",
        "\n",
